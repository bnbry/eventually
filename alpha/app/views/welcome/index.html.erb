<%= form_with model: @setting do |form| %>
  <%= form.label :fee_enabled %>
  <%= form.check_box :fee_enabled %>

  <hr>

  <%= form.label :discount_enabled %>
  <%= form.check_box :discount_enabled %>

  <hr>

  <%= form.label :version %>
  <%= @setting.version %>

  <hr>

  <%= form.label :guid %>
  <%= @setting.guid %>

  <hr>

  <%= form.submit %>
<% end %>
